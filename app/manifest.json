{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "default_locale": "en",
  "description": "翻译来自Bing词典，可自动识别中英互译，可将翻译结果直接插入到页面内容之中",
  "short_name": "__MSG_shortName__",
  "version": "1.2.1",
  "icons": {
    "128": "icon.png"
  },
  "author": "Dremy",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["chrome-dict.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}