{
  "manifest_version": 2,
  "name": "Chrome Dict",
  "version": "0.3.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["chrome-dict.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}